{
  "name": "bmad-studio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron:compile": "tsc --project electron/tsconfig.json",
    "electron:dev": "npm run electron:compile && concurrently \"vite\" \"wait-on http://localhost:3007 && electron .\"",
    "electron:build": "npm run backend:build && npm run build && npm run electron:compile && electron-builder",
    "backend:build": "bash scripts/build-backend.sh current",
    "dist": "npm run backend:build && npm run build && npm run electron:compile && electron-builder",
    "dist:mac": "npm run backend:build && npm run build && npm run electron:compile && electron-builder --mac",
    "dist:linux": "npm run backend:build && npm run build && npm run electron:compile && electron-builder --linux"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dompurify": "^3.3.1",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.563.0",
    "marked": "^17.0.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/dompurify": "^3.0.5",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "concurrently": "^9.1.2",
    "electron": "^35.1.2",
    "electron-builder": "^26.0.12",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.4.0",
    "vite": "^5.1.0",
    "wait-on": "^8.0.3"
  }
}
